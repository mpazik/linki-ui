{
  "name": "linki-ui",
  "version": "0.0.4",
  "description": "linki dom render component and set of utilities to integrate with dom API",
  "keywords": [
    "front-end",
    "vdom",
    "browser",
    "callback",
    "dataflow",
    "framework"
  ],
  "author": "Marek Pazik <contact@marekpazik.pl>",
  "license": "mit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpazik/linki-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mpazik/linki-ui/issues"
  },
  "homepage": "https://github.com/mpazik/linki-ui#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "yarn eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "linki": "^0.1.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "esbuild": "^0.14.6",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "jest": "^27.4.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)s$": ["esbuild-jest"]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!linki)"
    ]
  }
